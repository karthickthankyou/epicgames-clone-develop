  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --coverage --detectOpenHandles",
    "test:e2e": "cypress open",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "type:check": "tsc -p tsconfig.json",
    "lint": "eslint --ext .ts --ext .tsx .",
    "validate": "npm-run-all --parallel type:check format:check lint",
    "prepare": "husky install",
    "commit": "npx cz",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "depcheck": "npx depcheck",
    "build:deptree": "depcruise --exclude '(node_modules|__tests__|dist)'  | dot -T svg > static/img/dependencygraph.svg"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addons": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/cypress": "^8.0.0",
    "@types/jest-axe": "^3.5.3",
    "@types/lodash.debounce": "^4.0.6",
    "@types/react-helmet": "^6.1.2",
    "@types/react-slider": "^1.3.1",
    "@types/styled-components": "^5.1.15",
    "@types/tailwindcss": "^2.2.2",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "autoprefixer": "^9.8.6",
    "commitizen": "^4.2.4",
    "cypress": "^8.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dependency-cruiser": "^10.5.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.1",
    "jest-axe": "^5.0.1",
    "lint-staged": "^11.1.1",
    "postcss": "^7.0.36",
    "prettier": "^2.3.2",
    "storybook-addon-deps": "^2.2.0",
    "storybook-addon-designs": "^6.2.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.7"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,js,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
